  id                   String          @id @default(uuid())
  storeId              String
  store                Store           @relation("StoreToWatch", fields: [storeId], references: [id])
  categoryId           String
  category             Category        @relation("CategoryWatch", fields: [categoryId], references: [id])
  name                 String
  heading              String
  description          String
  // Thông tin sản phẩm bảo hanh vat ...
  headingrecommend     String
  infomationrecommend  String
  warrantyrecommend    String
  vatrecommend         String
  // Khuyến mãi
  promotionheading     String          @db.VarChar(400)
  promotiondescription String          @db.VarChar(400)
  //Bảo hành
  guaranteeheading     String          @db.VarChar(400)
  guaranteedescription String          @db.VarChar(400)
  guaranteeinfomation  String          @db.VarChar(400)
  guaranteeprice       String          @db.VarChar(400)
  //Giá sản phẩm main
  price                Decimal
  priceold             Decimal
  percentpromotion     Decimal
  isFeatured           Boolean         @default(false)
  isArchived           Boolean         @default(false)
  sizeId               String
  size                 Size            @relation(fields: [sizeId], references: [id])
  colorId              String
  color                Color           @relation(fields: [colorId], references: [id])
  imageswatch          ImageWatch[]
  specificationsId     String
  specifications       Specifications  @relation(fields: [specificationsId], references: [id])
  salientfeaturesId    String
  salientfeatures      Salientfeatures @relation(fields: [salientfeaturesId], references: [id])
  orderItems           OrderItem[]
  createdAt            DateTime        @default(now())
  updatedAt            DateTime        @updatedAt

  @@index([specificationsId])
  @@index([salientfeaturesId])
  @@index([storeId])
  @@index([categoryId])
  @@index([sizeId])
  @@index([colorId])
}

